{
  "name": "demo",
  "description": "A demonstration of the saturn.json schema",
  "image_uri": "public.ecr.aws/saturncloud/saturn:2022.01.06",
  "extra_packages": {
    "conda": {"install": "tensorflow==2.6.0 papermill", "use_mamba": false}
  },
  "environment_variables": {
    "VAR1": "VALUE1",
    "VAR2": "VALUE2"
  },
  "start_script": "pip install torch",
  "jupyter_server": {
    "instance_type": "xlarge",
    "start_ssh": true,
    "disk_space": "10Gi",
    "auto_shutoff": "1 hour"
  },
  "dask_cluster": {
    "num_workers": 3,
    "scheduler": {
      "instance_type": "xlarge"
    },
    "worker": {
      "instance_type": "xlarge",
      "use_spot_instances": true
    }
  },
  "git_repositories": [
    {
      "url": "git@github.com:tmobile/r-tensorflow-api.git"
    },
    {
      "url": "git@github.com:jnolis/pet-names.git",
      "on_restart": "preserve changes",
      "reference_type": "branch",
      "reference": "dev"
    }
  ],
  "schema_version": "2022.06.01"
}

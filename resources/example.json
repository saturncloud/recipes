{
  "name": "demo",
  "description": "A demonstration of the saturn.json schema",
  "image_uri": "saturncloud/saturn:2021.11.10",
  "extra_packages": {
    "conda": {"install": "tensorflow==2.6.0 papermill", "use_mamba": false}
  },
  "environment_variables": {
    "VAR1": "VALUE1",
    "VAR2": "VALUE2"
  },
  "start_script": "pip install torch",
  "jupyter_server": {
    "instance_type": "xlarge",
    "start_ssh": true,
    "disk_space": "10gb",
    "auto_shutoff": "1 hour"
  },
  "dask_cluster": {
    "num_workers": 3,
    "scheduler": {
      "instance_type": "xlarge"
    },
    "worker": {
      "instance_type": "xlarge"
    }
  },
  "git_repositories": [
    {
      "url": "git@github.com:tmobile/r-tensorflow-api.git"
    },
    {
      "url": "git@github.com:jnolis/pet-names.git",
      "on_restart": "preserve changes",
      "reference_type": "branch",
      "reference": "dev"
    }
  ],
  "shared_folders": [
    {
      "owner": "alex",
      "name": "data"
    },
    {
      "owner": "julia",
      "name": "data",
      "path": "/home/jovyan/shared/my-data"
    }
  ],
  "credentials": [
    {
      "name": "snowflake_key",
      "type": "file",
      "location": "/snowflake.txt"
    },
    {
      "name": "snowflake_password",
      "type": "environment_variable",
      "location": "SNOWFLAKE_PASSWORD"
    }
  ],
  "schema_version": "main"
}
